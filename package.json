{
  "name": "vue-game",
  "version": "1.0.0",
  "description": "this is a vue project for gaming",
  "main": "main.js",
  "scripts": {
    "start": "browser-sync start --server dist/",
    "dev": "webpack-dev-server --host 0.0.0.0 --inline --progress --compress --profile --colors --watch --hot --display-error-details --content-base dist/",
